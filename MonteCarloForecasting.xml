<script>

function reemplazar() {
 var divs = document.getElementsByTagName("div");
     for (var i = 0; i < divs.length; i++) {
            var para = divs[i].getElementsByTagName("div");
            if ( para[0] ) {
            //alert(para.length)
                if (para.length  == 3 ){
                  divs[i].id = "mraddon";  
                  divs[i].style.visibility = "hidden";                  
                  divs[i].innerHTML = "";
                }
            }
      } 
document.body.innerHTML = document.body.innerHTML.replace(/Powered by CoinMarketCap/g, '');
}

